 <div class="jumbotron centered">
    <div class="container"> 
       
       <hr>
       <div style="align-content: flex-start;">
        <a href="/users/sign-up"  class="btn btn-dark btn-lg active" role="button" aria-pressed="true">Employee</a>
        <a href="/company/sign-up"  class="btn btn-light btn-lg active" role="button" aria-pressed="true">Recruiter</a>
        </div>
       
        
            <form action="/users/create" method="POST">
                <label for="inputEmail" class="sr-only">Name</label>
                <input type="name" name="name" class="form-control" placeholder="Name" required autofocus>
        
                <label for="inputEmail" class="sr-only">Email address</label>
                <input type="email" name="email" class="form-control" placeholder="Email address" required autofocus>
             
                 <label for="inputPassword" class="sr-only">Password</label>
                <input type="password" pattern=".{6,}" name="password" class="form-control" placeholder="Password" required>
                   
                <label for="inputPassword" class="sr-only">Confirm Password</label>
                <input type="password" pattern=".{6,}" name="confirm_password" class="form-control" placeholder="Confirm Password" required>
                
                <label for="prior_experience" class="sr-only">Prior_Experience </label>
                <input type="number" name="prior_experience" placeholder="Exp. In Years" class="form-control">
                
                <label for="interest" class="sr-only">Interest</label>
                <input type="text" name="interest" class="form-control" placeholder="field of interest or skill" required>

                <label for="q1" class="sr-only">Q1 </label>
                <input type="number" name="q1" placeholder="1. Find next number in the sequence 2, 5, 10, 17, 26,?" class="form-control">

                <label for="q2" class="sr-only">Q2 </label>
                <input type="number" name="q2" placeholder="2. Find next number in the sequence 1,4,9,16,25,?" class="form-control">

                <label for="q3" class="sr-only">Q3 </label>
                <input type="number" name="q3" placeholder="3. Find next number in the sequence 13,16,22,34,58,?" class="form-control">

                <input type="hidden" value="true" name="isuser">
                <input class="btn btn-dark btn-lg" type="submit" value="Sign Up">
            </form>
        </div>
      
  
    </div>
  

